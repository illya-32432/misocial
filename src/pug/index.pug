doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel="stylesheet", href="css/style.css")
        title misocial
    body 
        include components/mixin
        header 
            +menu(mainMenu)
            img.fon(src="../img/fonfirst.svg", alt="fon")
        .cards
            +card(cardsInfo.satelite)
            +card(cardsInfo.mars)
            +card(cardsInfo.rocket)

        footer 
            +menu(mainMenu, "dark")

        script(src="js/script.js") 